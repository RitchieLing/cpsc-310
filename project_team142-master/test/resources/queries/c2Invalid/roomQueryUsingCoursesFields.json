{
	"title": "room query using courses fields pass",
	"input": {
		"WHERE": {
			"AND": [{
				"IS": {
					"rooms_furniture": "*Tables*"
				}
			}, {
				"GT": {
					"rooms_pass": 300
				}
			}]
		},
		"OPTIONS": {
			"COLUMNS": [
				"rooms_shortname",
				"maxSeats"
			],
			"ORDER": {
				"dir": "DOWN",
				"keys": ["maxSeats"]
			}
		},
		"TRANSFORMATIONS": {
			"GROUP": ["rooms_shortname"],
			"APPLY": [{
				"maxSeats": {
					"MAX": "rooms_seats"
				}
			}]
		}
	},
	"errorExpected": true,
	"with": "InsightError"
}
